{
  "name": "egg-simple",
  "version": "1.0.0",
  "description": "about egg server",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "ramda": "^0.27.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-simple",
    "stop": "egg-scripts stop --title=egg-server-egg-simple",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint-fix": "eslint . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "m:new": "sequelize migration:create",
    "m:up": "sequelize db:migrate",
    "m:down": "sequelize db:migrate:undo"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "k",
  "license": "MIT"
}
